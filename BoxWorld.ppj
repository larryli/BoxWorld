SIGN = posign.exe#
CCFLAGS = -Tx86-coff -Ot -W1 -Gz -Ze -Go #
ASFLAGS = -AIA32 -Gz#
RCFLAGS = -r#
LINKFLAGS = -machine:ix86 -subsystem:windows kernel32.lib user32.lib gdi32.lib comctl32.lib comdlg32.lib advapi32.lib delayimp.lib#
